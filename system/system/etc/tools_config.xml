		</loop>

		<loop name="5_Sleep_Current_Test_5-3" priority="30" cycle="1">

			<SuspendResume name="sleep_current_test.spspend_resume" enable="true" priority="5" cycle="1" s_interval="120" r_interval="1"/>

		</loop>

	</loop>



	<loop name="Step6" enable="true" priority="960" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="true" priority="9" value="disable"/>

		<FrontCameraPic name="front_cam_pic" enable="true" priority="10" interval="9" period="300">
			<PreviewOnly>false</PreviewOnly>
			<ZoomSwitch>true</ZoomSwitch>
			<Resolution enable="true" width="640" height="480"/>
			<PicPath enable="true">/sdcard/Movies/front_cam_pic.jpg</PicPath>
		</FrontCameraPic>
		<Delay name="delay" enable="true" priority="2" period="5"/>

		<MainCameraPic name="main_cam_pic" enable="true" priority="15" interval="9" period="300">
			<FlashOn>false</FlashOn>
			<PreviewOnly>false</PreviewOnly>
			<ZoomSwitch>true</ZoomSwitch>
			<Resolution enable="true" width="1024" height="768"/>
			<PicPath enable="true">/sdcard/Movies/main_cam_pic.jpg</PicPath>
		</MainCameraPic>
		<Delay name="delay" enable="true" priority="2" period="5"/>
		<MainCameraPic name="main_cam_pic_flashOn" enable="true" priority="20" interval="9" period="300">
				<FlashOn>true</FlashOn>
				<PreviewOnly>false</PreviewOnly>
				<ZoomSwitch>true</ZoomSwitch>
				<Resolution enable="true" width="1024" height="768"/>
				<PicPath enable="true">/sdcard/Movies/main_cam_flash_pic.jpg</PicPath>
		</MainCameraPic>
		<Delay name="delay" enable="true" priority="2" period="5"/>

		<FrontCameraVideo name="front_cam_video" enable="false" priority="23" period="300">
			<Resolution enable="true" width="640" height="480"/>
  			<RecordPath>/sdcard/Movies/front_cam_rec.mp4</RecordPath>
 		</FrontCameraVideo>
		<Delay name="delay" enable="true" priority="2" period="5"/>

		<MainCameraVideo name="main_cam_video" enable="true" priority="25" period="300">
			<Resolution enable="true" width="640" height="480"/>
			<RecordPath>/sdcard/Movies/main_cam_video.mp4</RecordPath>
		</MainCameraVideo>

		<CameraSwitch name="cam_switch" enable="false" priority="30" interval="9" period="300"/>

	</loop>

	<loop name="Step7" enable="true" priority="970" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="true" priority="1" value="enable"/>



		<EMMC name="emmc" enable="true" priority="7" period="1200"/>

	</loop>
	<loop name="Step8" enable="true" priority="980" cycle="1">
	<SetChargingMode name="set_charging_mode" enable="true" priority="1" value="enable"/>
	</loop>
</alt_config>
